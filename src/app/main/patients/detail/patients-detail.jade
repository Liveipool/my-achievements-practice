include ./partials/patients-detail-basic-information.jade
include ./partials/patients-detail-acography.jade
include ./partials/patients-detail-inspection-record.jade
include ./partials/patients-detail-followups.jade

mixin header
  .header.md-accent-bg(layout='row', layout-align='space-between')
    div(layout='column', layout-align='center start')
      .breadcrumb(layout='row', layout-align='start center')
        span.parent 患者一览表
        md-icon.s16.separator(md-font-icon='icon-chevron-right')
      .title {{pd.name}}患者详情

mixin content
  md-content.md-padding
    .white-bg.md-whiteframe-4dp
      md-tabs(md-dynamic-height='')
        +basic-information
        +acography
        +inspection-record
        +followups

#simple-table.page-layout.simple.fullwidth.doc-page.element-doc-page

  +header
  +content




















